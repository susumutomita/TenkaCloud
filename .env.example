# PostgreSQL Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB=tenkacloud

# Database URL (used by Prisma and backend services)
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgres:5432/${POSTGRES_DB}?schema=public

# CORS Allowed Origins (comma-separated)
ALLOWED_ORIGIN=http://localhost:3000,http://localhost:3001,http://localhost:3002,http://localhost:3003

# Keycloak Configuration
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Auth Secrets (generate with: openssl rand -base64 32)
AUTH_SECRET=your-secret-key-here
AUTH_KEYCLOAK_SECRET=your-keycloak-client-secret-here

# NOTE: Copy this file to .env and update with your values:
# cp .env.example .env
