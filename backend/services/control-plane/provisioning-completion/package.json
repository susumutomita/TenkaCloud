{
  "name": "@tenkacloud/provisioning-completion",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun build src/handler.ts --outdir=dist --target=node --format=esm",
    "deploy": "bun run build && cd dist && zip -r ../lambda.zip .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/lib-dynamodb": "^3.700.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  }
}
