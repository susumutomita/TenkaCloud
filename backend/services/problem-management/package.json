{
  "name": "@tenkacloud/problem-management",
  "version": "0.1.0",
  "description": "TenkaCloud Problem Management Service - GameDay/JAM問題管理、マルチクラウドデプロイ、採点エンジン",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun run --watch src/server.ts",
    "start": "bun run src/server.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.1",
    "@prisma/client": "^5.22.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "hono": "^4.6.10",
    "jose": "^5.9.6",
    "yaml": "^2.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^9.15.0",
    "prisma": "^5.22.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "@aws-sdk/client-cloudformation": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-sts": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-cloudformation": {
      "optional": true
    },
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@aws-sdk/client-lambda": {
      "optional": true
    },
    "@aws-sdk/client-sts": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "tenkacloud",
    "gameday",
    "jam",
    "cloud",
    "aws",
    "gcp",
    "azure",
    "competition",
    "scoring"
  ],
  "author": "TenkaCloud Team",
  "license": "MIT"
}
